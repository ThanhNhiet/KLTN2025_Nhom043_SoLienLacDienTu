{
  "expo": {
    "name": "Coolking-Econtact",
    "slug": "Coolking-Econtact",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.jpg",
    "scheme": "coolkingecontact",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
     "android": {
      "package": "com.coolking.econtact",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "POST_NOTIFICATIONS",
        "VIBRATE",
        "WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.jpg",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/logo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        { "image": "./src/assets/images/logo.jpg", "imageWidth": 200, "resizeMode": "contain", "backgroundColor": "#ffffff", "dark": { "backgroundColor": "#000000" } }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/images/logo.jpg",   
          "color": "#1e88e5",
          "sounds": []
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8361f42f-4a9e-4e2f-b934-d376d7940375"
      }
    },
    "owner": "tuanvuongg"
  }
}
